<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="amazon test">
    <parameter name="useCloudEnv" value="false"></parameter>

    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="100"></parameter>

    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://www.amazon.com/"></parameter>
    <test name="amazon tests">
        <classes>
            <!--            <class name="test.TestLogin"></class>-->
            <class name="test.TestSearch"></class>
            <class name="test.TestPurchase"></class>
        </classes>
    </test>
</suite>